{
    "multipart": [
        { "apply": { "model": "terra_reforged:block/plant_thorn_jungle_core" }},				
				{   "when": { "north": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "uvlock": true }
        },
				{   "when": { "north": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "uvlock": true }
        },
				{   "when": { "south": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "y": 180, "uvlock": true }
        },
				{   "when": { "south": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "y": 180, "uvlock": true }
        },
				{   "when": { "east": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "y": 90, "uvlock": true }
        },
				{   "when": { "east": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "y": 90, "uvlock": true }
        },
				{   "when": { "west": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "y": 270, "uvlock": true }
        },
				{   "when": { "west": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "y": 270, "uvlock": true }
        },
				{   "when": { "up": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "x": 270, "uvlock": true }
        },
				{   "when": { "up": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "x": 270, "uvlock": true }
        },
				{   "when": { "down": "true" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side", "x": 90, "uvlock": true }
        },
				{   "when": { "down": "false" },
            "apply": { "model": "terra_reforged:block/plant_thorn_jungle_side_off", "x": 90, "uvlock": true }
        }
    ]
}
