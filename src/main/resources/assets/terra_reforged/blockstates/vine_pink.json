{
    "multipart": [
        {   "when": { "north": "true" },
            "apply": { "model": "terra_reforged:block/vine_pink", "y": 180, "uvlock": true }
        },
				{   "when": { "south": "true" },
            "apply": { "model": "terra_reforged:block/vine_pink", "uvlock": true }
        },
				{   "when": { "east": "true" },
            "apply": { "model": "terra_reforged:block/vine_pink", "y": 270, "uvlock": true }
        },
				{   "when": { "west": "true" },
            "apply": { "model": "terra_reforged:block/vine_pink", "y": 90, "uvlock": true }
        },
				{   "when": { "up": "true" },
            "apply": { "model": "terra_reforged:block/vine_pink", "x": 90, "uvlock": true }
        }
    ]
}
