{
    "multipart": [
				{   "when": { "up": "none"},
            "apply": { "model": "terra_reforged:block/wall_brick_post" }
        },
				{   "when": { "up": "pillar"},
            "apply": { "model": "terra_reforged:block/wall_brick_pillar" }
        },
				{   "when": { "up": "up"},
            "apply": { "model": "terra_reforged:block/wall_brick_post_up" }
        },
				{   "when": { "north": "side"},
            "apply": { "model": "terra_reforged:block/wall_brick_side", "uvlock": true }
        },
				{   "when": { "east": "side"},
            "apply": { "model": "terra_reforged:block/wall_brick_side", "y": 90, "uvlock": true }
        },
				{   "when": { "south": "side"},
            "apply": { "model": "terra_reforged:block/wall_brick_side", "y": 180, "uvlock": true }
        },
				{   "when": { "west": "side"},
            "apply": { "model": "terra_reforged:block/wall_brick_side", "y": 270, "uvlock": true }
        },
				{   "when": { "north": "up"},
            "apply": { "model": "terra_reforged:block/wall_brick_side_up" }
        },
				{   "when": { "east": "up"},
            "apply": { "model": "terra_reforged:block/wall_brick_side_up", "y": 90, "uvlock": true }
        },
				{   "when": { "south": "up"},
            "apply": { "model": "terra_reforged:block/wall_brick_side_up", "y": 180, "uvlock": true }
        },
				{   "when": { "west": "up"},
            "apply": { "model": "terra_reforged:block/wall_brick_side_up", "y": 270, "uvlock": true }
        }
    ]
}
