{
  "multipart":
	[
    { "when": { "up": "true" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_post" }},
		{ "when": { "north": "low" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side", "uvlock": true }},
    { "when": { "east": "low" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side", "y": 90, "uvlock": true }},
    { "when": { "south": "low" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side", "y": 180, "uvlock": true }},
    { "when": { "west": "low" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side", "y": 270, "uvlock": true }},
    { "when": { "north": "tall" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side_up", "uvlock": true }},
    { "when": { "east": "tall" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side_up", "y": 90, "uvlock": true }},
    { "when": { "south": "tall" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side_up", "y": 180, "uvlock": true }},
    { "when": { "west": "tall" }, "apply": { "model": "terra_reforged:block/wall_oak_leaf_side_up", "y": 270, "uvlock": true }}
  ]
}